/* pages/common/report/report.wxss */

.bt {
  text-align: center;
  display: block;
  margin: 0 auto;
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  padding: 30rpx 0;
}
.ul{
  border-top: 1px solid #eee;
}
.li {
  display: flex;
  justify-content: space-between;
  padding: 28rpx 30rpx;
  border-bottom: 1px solid #eee;
}

.li text {
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.li image {
  width: 16rpx;
  height: 26rpx;
}
